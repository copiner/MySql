select c.cust_id, c.fed_id,
case when c.cust_type_cd = 'I' then
     (select concat(i.fname,' ',i.lname)
     from individual i
     where i.cust_id = c.cust_id)
     when c.cust_type_cd = 'B' then
     (select b.name
     from business b
     where b.cust_id = c.cust_id)
     else 'nonono'
end name
from customer c;
